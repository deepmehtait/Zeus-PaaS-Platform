    <script type="text/javascript" >
    
    function getFileNames(data){
    var length = data.length;

    for(var i =0;i < length;i++)
    {
    if ( data[i].d != undefined) {

    var fileLoc = data[i].d.split('/');
    
    var createFolder='<ol class="dd-list">'+
'<li class="dd-item" data-id="1">'+
'<div class="dd-handle">'+fileLoc[fileLoc.length-1]+'</div>'+
'<ol class="dd-list">'+
'</ol></li></ol>';
$("#nestable").append(createFolder);
    }

    }
 
   
    for(var i =0;i < length;i++)
    {
   
        if ( data[i].f !=  undefined) {
     var filePath = data[i].f;
    var fileLoc = data[i].f.split('/');
    var createFiles='<ol class="dd-list">'+
'<li class="dd-item" data-id="1">'+
'<a href="#" id='+filePath+' onClick="getFileContent(this.id);"><li class="dd-item"  data-id="3">'+fileLoc[fileLoc.length-1]+'</li></a>'+
'</ol></li></ol>';
$("#nestable").append(createFiles);
    }
    }
    }
    function getFileContent(filePath){
    alert(filePath);
    
    var text_send = '"'+filePath+'"';
    var k = String(text_send);
    alert(text_send);
    var k  = '{"k":k}';
   // alert(k.text_send);
    $.ajax({
                        
                        type: 'get',
                        data: text_send,
                        dataType: "json",
                        contentType: 'application/json',
                        url: '/readFromFile?fileDest='+text_send,                       
                        success: function(data) {
                           getFileNames(data);
                           $("#code").val(data);
                        
                        }
                        
                    });
        
   
    
    }
    </script>
    
